{
  "schemaVersion": 1,
  "id": "meteor-extra",
  "version": "${version}",
  "name": "Meteor Extra",
  "description": "Meteor Extra is a template addon for Meteor Client.",
  "authors": [
    "AntiSea",
    "sea"
  ],
  "contact": {
    "repo": "https://github.com/AntiSea/MeteorExtra"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "net.antisea.meteorextra.MeteorExtra"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
